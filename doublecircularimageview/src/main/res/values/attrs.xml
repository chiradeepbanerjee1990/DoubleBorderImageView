<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DoubleCircularImageView">
        <attr name="outerciclecolor" format="color"/>
        <attr name="innercirclecolor" format="color"/>

        <attr name="outerradius" format="integer"/>
        <attr name="innerradius" format="integer"/>

        <attr name="outerradiuswidth" format="integer"/>
        <attr name="innerrardiuswidth" format="integer"/>

        <attr name="cliptype" format="enum">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>
</resources>